  "description": "homebridge-plugin for ZWay Server and RaZBerry",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmichas/homebridge-zway-plugin.git"
  },
  "preferGlobal": true,
  "keywords": [
    "homebridge-plugin",
    "Z-Way",
    "RaZBerry",
    "Z-Wave",
    "HomeKit"
  ],
  "engines": {
