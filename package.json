    "eslint": "^8.0.1",
    "homebridge": "^1.3.5",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4",
    "nyc": "*",
